{
  "source": {
      "includePattern": ".+\\.js(doc|x)?$",   
      "include": ["."],                       
      "exclude": ["node_modules"]             
  },
  "recurseDepth": 10,
  "plugins": [
    "plugins/markdown"
  ],                           
  "opts": {    
      "destination": "./docs/",               
      "recurse": true                         
  }
}


